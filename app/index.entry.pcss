  }

  & img {
    width: 72px;
    height: 72px;
    transition: opacity 0.15s ease-in-out;


    @media (--sm) {
      &[alt="re-sharper"] {
        grid-area: resharper;
      }

      &[alt="resharper-c"] {
        grid-area: resharper-c;
      }

      &[alt="dot-trace"] {
        grid-area: dottrace;
      }

      &[alt="dot-cover"] {
        grid-area: dotcover;
      }

      &[alt="dot-memory"] {
        grid-area: dotmemory;
      }
    }
  }

  & .extensions-text {
    grid-row: 2;
    display: grid;
    grid-template-columns: repeat(2, 1fr);
    row-gap: 30px;
    column-gap: 100px;
  }

  @media (--lg) {
    justify-content: space-between;
    column-gap: normal;
  }

  & .extensions-text-item {
    display: flex;
    flex-direction: column;
    gap: 6px;

