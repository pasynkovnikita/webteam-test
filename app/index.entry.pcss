      line-height: 1.4;
      letter-spacing: normal;
      color: #27282c;
    }

    & .socials-subscribe {
      display: flex;
      flex-direction: column;
      row-gap: 24px;

      & form {
        display: flex;
        flex-direction: column;
        gap: 24px;
        flex-wrap: wrap;
        justify-content: start;
        align-items: flex-start;

        & input {
          background: url("static/pics/socials/enter.png") no-repeat right 16px center;
          background-size: 24px;
        }

        & #email {
          width: 100%;
          height: 48px;
          border-radius: 4px;
          border: solid 1px rgba(39, 40, 44, 0.2);
          background-color: #fff;
          padding: 12px 16px;

          &::placeholder {
            font-family: SFProText, sans-serif;
            font-size: 15px;
            font-weight: normal;
            font-stretch: normal;
            font-style: normal;
            line-height: 1.6;
            letter-spacing: normal;
            color: rgba(39, 40, 44, 0.7);
          }
        }

        & .policy {
          display: flex;
          flex-direction: row;
          gap: 20px;
          align-items: flex-start;

          & label {
            width: 75%;
            font-family: SFProText, sans-serif;
            font-size: 12px;
            font-weight: normal;
            font-stretch: normal;
            font-style: normal;
            line-height: 1.33;
            letter-spacing: normal;
            color: rgba(39, 40, 44, 0.7);
          }
        }

        button {
          display: inline-flex;
          border: none;
          padding: 12px 40px;
          border-radius: 25px;
          background-color: #f76707;
          font-family: SFProText, sans-serif;
          font-size: 15px;
          font-weight: normal;
          font-stretch: normal;
          font-style: normal;
          line-height: 1.6;
          letter-spacing: normal;
          text-align: center;
          color: #fff;
          transition: background-color 0.2s ease-in-out;
          cursor: pointer;

          &:hover {
            background-color: #dc5b06;
          }
        }
      }
    }

    & .socials-follow {
      display: flex;
      flex-direction: column;
      row-gap: 24px;

      & .social-item {
        display: flex;
        flex-direction: row;
        gap: 16px;

        img {
          object-fit: contain;
        }

        & a:hover {
          color: #dc5b06;
        }
      }

    }
  }
}

a {
  font-family: SFProText, sans-serif;
  font-size: 18px;
  font-weight: normal;
  font-stretch: normal;
  font-style: normal;
  line-height: 1.56;
  letter-spacing: normal;
  color: #f76707;
  text-decoration: none;
  transition: color .2s ease-in-out;

  &:active {
    color: #f76707;
  }
}

footer {
  background-color: #27282c;
  padding: 48px;

  & .container {
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: flex-start;
    gap: 64px;

    @media (--sm) {
      flex-direction: column;
      gap: 32px;
    }

    & a {
      font-family: SFProText, sans-serif;
      font-size: 15px;
      font-weight: normal;
      font-stretch: normal;
      font-style: normal;
      line-height: 1.6;
      letter-spacing: normal;
      color: rgba(255, 255, 255, 0.6);
    }

    & p {
      font-family: SFProText, sans-serif;
      font-size: 15px;
      font-weight: bold;
      font-stretch: normal;
      font-style: normal;
      line-height: 1.6;
      letter-spacing: normal;
      color: #fff;
    }

    & img {
      object-fit: contain;

      &[alt="logo"] {
        @media (--sm) {
          display: none;
        }
      }

      &[alt="logo-sm"] {
        display: none;

        @media (--sm) {
          display: block;
        }
      }


    }

    & .about {
      display: flex;
      flex-direction: column;

      & p {
        padding-bottom: 8px;
      }
    }

    & .follow {
      display: flex;
      flex-direction: column;

      & p {
        padding-bottom: 24px;
      }


      & .follow-logos {
        display: flex;
        flex-direction: row;
        align-items: center;
        gap: 24px;
      }
    }
  }

  & .rights {
    display: flex;
    flex-direction: row;
    gap: 16px;
    align-items: baseline;
    justify-content: flex-start;
    font-family: SFProText, sans-serif;
    font-size: 15px;
    font-weight: normal;
    font-stretch: normal;
    font-style: normal;
    line-height: 1.6;
    letter-spacing: normal;
    color: rgba(255, 255, 255, 0.3);

    & a {
      font-family: SFProText, sans-serif;
      font-size: 15px;
      font-weight: normal;
      font-stretch: normal;
      font-style: normal;
      line-height: 1.6;
      letter-spacing: normal;
      color: #fff;
    }

    @media (--sm) {
      flex-direction: column;
      gap: 12px;
    }
  }
}
